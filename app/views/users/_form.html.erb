<%= form_for @user do |f| %>
 <div class="field">
  <%= f.label:name %>
  <%= f.text_field :name, placeholder: "Your name"%>
 </div>
 
 <div class="field">
  <%= f.label :email, "Email" %>
  <%= f.text_field :email, placeholder: "Your Email" %>
 </div>
 
 <div class="field">
  <%= f.label :password, "Password" %>
  <%= f.password_field :password, placeholder: "Your Password" %>
 </div>
 
 <div class="field">
  <%= f.label :password_confirmation, "Type same Password" %>
  <%= f.password_field :password_confirmation, placeholder: "Password Confirmation" %>
 </div>
 

 <div class="field">
  <%= f.label:career_year, 'Your Career' %>
  <%= select(:user, :career_year, [['Nothing/Self-Learning ONLY'], ['1 year'],['2 years'],['3 years'],['4 years'],['5 years'],['6 years'],['7 years'],['8 years'],['9 years'],['10 years'],['11 years'],['12 years'],['13 years'],['14 years'],['15 years'],['16 years'],['17 years'],['18 years'],['19 years'],['20 years'],['More Than 20 years']]) %>
 </div>
 
 

 <%= f.collection_check_boxes :tech_ids, Tech.all, :id, :name do |cb| %>
  <% cb.label(class: "checkbox-inline input_checkbox") {cb.check_box(class: "checkbox") + cb.text} %>
 <% end %>
 
 
 <%= f.file_field :image %>
 
  
 <div class="actions">
  <%= f.submit "Register" %>
 </div>
 
<% end %>
<%= link_to "Go back to Top Page", root_path %>