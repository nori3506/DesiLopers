<%= form_with(model: form, url: url, local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

	<%= f.label :zip %>
  <%= f.text_area :zip %>

  <%= f.label :address %>
  <%= f.text_area :address %>

  <%= f.label :phone %>
  <%= f.text_area :phone %>

  <%= f.submit "Create" %>

  <% end %>
